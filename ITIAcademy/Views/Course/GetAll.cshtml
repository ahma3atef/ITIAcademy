@model List<Course>
<a class="btn btn-primary" href="/Course/GoToAddForm/" role="button">Add New Course</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">CourseName</th>

            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
            <th scope="col">Details</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.CourseName</td>

                <td>
                    <a class="btn btn-primary" href="/Course/GoToEditForm/@item.Id">Edit</a>
                </td>
                <td>
                    <a class="btn btn-primary" href="/Course/Delete/@item.Id">Delete</a>
                </td>
                <td>
                    <a class="btn btn-primary" href="/Course/GetById/@item.Id">MoreDetails</a>
                </td>

            </tr>
        }


    </tbody>
</table>
