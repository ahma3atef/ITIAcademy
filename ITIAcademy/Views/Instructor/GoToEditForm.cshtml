@model Instructor
<form asp-action="SaveEditFormData" asp-controller="Instructor">
    <div class="form-row">
        <div class="form-group col-md-6">
            <label asp-for="Id">Id</label>
            <input type="number" class="form-control" asp-for=Id placeholder="Id" value="@Model.Id">
            <span asp-validation-for="Id" class="text-danger"></span>
        </div>

        <div class="form-group col-md-6">
            <label asp-for="FName">FirstName</label>
            <input type="text" class="form-control" asp-for="FName" placeholder="FName" value="@Model.FName"> 
            <span asp-validation-for="FName" class="text-danger"></span>


        </div>

        <div class="form-group col-md-6">
            <label asp-for="LName">LastName</label>
            <input type="text" class="form-control" asp-for="LName" placeholder="LName" value="@Model.LName">
            <span asp-validation-for="LName" class="text-danger"></span>
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Edit</button>
</form>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
